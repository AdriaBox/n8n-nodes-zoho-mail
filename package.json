{
  "name": "n8n-nodes-zoho-mail",
  "version": "0.1.0",
  "description": "n8n čvor za integraciju s Zoho Mail API-jem koristeći OAuth2 autentikaciju",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "zoho",
    "zoho-mail",
    "email",
    "oauth2"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"**/*.ts\" --fix",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ZohoMailOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ZohoMail/ZohoMail.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "*",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.1.1",
    "typescript": "*"
  },
  "dependencies": {
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  }
} 